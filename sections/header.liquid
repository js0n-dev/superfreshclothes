{%- if request.design_mode -%}
  <script>
    (function() {
      const events = [
        'shopify:section:select',
        'shopify:section:deselect'
      ];

      events.forEach(function(event) {
        
        document.addEventListener(event, function(e) {
          if(e.target.id !== 'shopify-section-mini-cart') return;

          const modal = e.target.getElementsByTagName('details-modal')[0];

          if (modal) {
            const modalDetails = e.target.querySelector('details');
            
            if((e.type === events[0] && !modalDetails.hasAttribute('open')) || (e.type === events[1] && modalDetails.hasAttribute('open'))) {
              const modalSummary = e.target.querySelector('summary');

              modalSummary.dispatchEvent(new Event('click'));
            }
          }
        });
      });
    })();
  </script>
{%- endif -%}


