{%- if settings.search_enabled and section.settings.icons_menu_type != 'popup' -%}
  {% capture search_html %}
    <details-modal class="header__search"{% if settings.search_desktop_style == 'clear_style' %} data-disable-element-focus="true"{% endif %}>
      <details>
        <summary class="header__icon header__icon--search header__icon--summary link focus-inset modal__toggle" aria-haspopup="dialog" aria-label="{{ 'general.search.search' | t }}">
          <span>
            <svg class="modal__toggle-open icon icon-search" aria-hidden="true" focusable="false" role="presentation">
              <use href="#icon-search">
            </svg>
            <svg class="modal__toggle-close icon icon-close" aria-hidden="true" focusable="false" role="presentation">
              <use href="#icon-close">
            </svg>
          </span>
        </summary>
        <div class="search-modal search-modal--{{ settings.search_desktop_style }} modal__content" role="dialog" aria-modal="true" aria-label="{{ 'general.search.search' | t }}">
          <div class="modal-overlay"></div>
          <div class="search-modal__content-wrapper page-width" tabindex="-1">
            {%- if settings.predictive_search_enabled -%}
              <predictive-search class="search-modal__form" data-loading-text="{{ 'accessibility.loading' | t }}">
            {%- endif -%}
            <div class="search-modal__content">
              <form action="{{ routes.search_url }}" method="get" role="search" class="search search-modal__form">
                <div class="search-modal__field search-modal__field--{{ settings.search_desktop_style }}">
                  {%- if settings.search_desktop_style == 'clear_style' -%}
                    <div class="field field--start small-hide medium-hide">
                      <input class="search__input field__input"
                        id="Search-In-Modal"
                        type="search"
                        name="q"
                        value=""
                        placeholder="{{ 'general.search.input_placeholder' | t }}"
                        {%- if settings.predictive_search_enabled -%}
                          role="combobox"
                          aria-expanded="false"
                          aria-owns="predictive-search-results-list"
                          aria-controls="predictive-search-results-list"
                          aria-haspopup="listbox"
                          aria-autocomplete="list"
                          autocorrect="off"
                          autocomplete="off"
                          autocapitalize="off"
                          spellcheck="false"
                        {%- endif -%}
                      >
                      <button type="reset" class="reset__button field__button{% if search.terms == blank %} hidden{% endif %}" aria-label="{{ 'general.search.reset' | t }}">
                        <svg class="icon icon-close" aria-hidden="true" focusable="false">
                          <use xlink:href="#icon-reset">
                        </svg>
                      </button>
                      <button class="search__button field__button" aria-label="{{ 'general.search.search' | t }}">
                        <svg class="icon icon-search" aria-hidden="true" focusable="false" role="presentation">
                          <use href="#icon-search">
                        </svg>
                      </button>
                      <label class="field__label" for="Search-In-Modal">{{ 'general.search.input_placeholder' | t }}</label>
                      <input type="hidden" name="options[prefix]" value="last">
                    </div>
                  {%- else -%}
                    <div class="field field--start">
                      <input class="search__input field__input"
                        id="Search-In-Modal"
                        type="search"
                        name="q"
                        value=""
                        placeholder="{{ 'general.search.search' | t }}"
                        {%- if settings.predictive_search_enabled -%}
                          role="combobox"
                          aria-expanded="false"
                          aria-owns="predictive-search-results-list"
                          aria-controls="predictive-search-results-list"
                          aria-haspopup="listbox"
                          aria-autocomplete="list"
                          autocorrect="off"
                          autocomplete="off"
                          autocapitalize="off"
                          spellcheck="false"
                        {%- endif -%}
                      >
                      <button type="reset" class="reset__button field__button{% if search.terms == blank %} hidden{% endif %}" aria-label="{{ 'general.search.reset' | t }}">
                        <svg class="icon icon-close" aria-hidden="true" focusable="false">
                          <use xlink:href="#icon-reset">
                        </svg>
                      </button>
                      <button class="search__button field__button" aria-label="{{ 'general.search.search' | t }}">
                        <svg class="icon icon-search" aria-hidden="true" focusable="false" role="presentation">
                          <use href="#icon-search">
                        </svg>
                      </button>
                      <label class="field__label" for="Search-In-Modal">{{ 'general.search.search' | t }}</label>
                      <input type="hidden" name="options[prefix]" value="last">
                    </div>
                    <button type="button" class="search-modal__close-button modal__close-button link link--text focus-inset" aria-label="{{ 'accessibility.close' | t }}">
                      <svg class="icon icon-close" aria-hidden="true" focusable="false" role="presentation">
                        <use href="#icon-close">
                      </svg>
                    </button>
                  {%- endif -%}
                </div>
                
                {%- if settings.predictive_search_enabled -%}
                  <div class="predictive-search predictive-search--header page-width" tabindex="-1" data-predictive-search>
                    <div class="predictive-search__loading-state">
                      <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                        <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                      </svg>
                    </div>
                  </div>

                  <span class="predictive-search-status visually-hidden" role="status" aria-hidden="true"></span>
                {%- endif -%}
              </form>
            </div>
            {%- if settings.predictive_search_enabled -%}
              </predictive-search>
            {%- endif -%}
          </div>
        </div>
      </details>
    </details-modal>
  {% endcapture %}
{%- endif -%}

{%- if section.settings.icons_menu != blank -%}
  {%- style -%}
    .header__icons-menu-item,
    .menu-drawer__icons-menu-item,
    .popup-menu-wrapper {
      {%- if section.settings.icons_menu_color.alpha > 0 -%}
        --icons-menu-color: {{ section.settings.icons_menu_color.red }}, {{ section.settings.icons_menu_color.green }}, {{ section.settings.icons_menu_color.blue }};
      {%- endif -%}
      {%- if section.settings.icons_menu_icon_color.alpha > 0 -%}
        --icons-menu-icon-color: {{ section.settings.icons_menu_icon_color.red }}, {{ section.settings.icons_menu_icon_color.green }}, {{ section.settings.icons_menu_icon_color.blue }};
      {%- endif -%}
      {%- if section.settings.icons_menu_border_color.alpha > 0 -%}
        --icons-menu-border: {{ section.settings.icons_menu_border_color.red }}, {{ section.settings.icons_menu_border_color.green }}, {{ section.settings.icons_menu_border_color.blue }};
      {%- endif -%}
      {%- if section.settings.icons_menu_hover_color.alpha > 0 -%}
        --icons-menu-hover-color: {{ section.settings.icons_menu_hover_color.red }}, {{ section.settings.icons_menu_hover_color.green }}, {{ section.settings.icons_menu_hover_color.blue }};
      {%- endif -%}
      {%- if section.settings.icons_menu_border_n_background_hover_color.alpha > 0 -%}
        --icons-menu-hover-background: {{ section.settings.icons_menu_border_n_background_hover_color.red }}, {{ section.settings.icons_menu_border_n_background_hover_color.green }}, {{ section.settings.icons_menu_border_n_background_hover_color.blue }};
        --icons-menu-hover-border: {{ section.settings.icons_menu_border_n_background_hover_color.red }}, {{ section.settings.icons_menu_border_n_background_hover_color.green }}, {{ section.settings.icons_menu_border_n_background_hover_color.blue }};
      {%- endif -%}
      {%- if section.settings.icons_menu_border_width > 0 -%}
        --icons-menu-border-width: {{ section.settings.icons_menu_border_width | divided_by: 10.0 }}rem;
      {%- endif -%} 
    }

    {%- if section.settings.popup_menu_height > 0 -%}
      .menu {
        --menu-height: {{ section.settings.popup_menu_height | divided_by: 10 }}rem !important;
      }
    {%- endif -%}

    .header__icons-menu-item,
    .popup-menu__link {
      {%- if section.settings.icons_menu_background_color.alpha > 0 -%}
        background: rgb({{ section.settings.icons_menu_background_color.red }}, {{ section.settings.icons_menu_background_color.green }}, {{ section.settings.icons_menu_background_color.blue }});
      {%- endif -%}
    }
    {%- if settings.enable_page_width and settings.page_width <= 1300 and section.settings.icons_menu.links.size > 8 -%}
      {%- assign icons_menu_limit_lg = 8 -%}
    {%- elsif section.settings.icons_menu.links.size > 12 -%}
      {%- assign icons_menu_limit_lg = 12 -%}
    {%- endif -%}
    {%- if section.settings.icons_menu.links.size > 8 -%}
      {%- assign icons_menu_limit_md = 8 -%}
      {%- assign half_links_size = section.settings.icons_menu.links.size | divided_by: 2 -%}
      {%- assign modulo_links_size = section.settings.icons_menu.links.size | modulo: 2 -%}
      {%- if modulo_links_size > 0 -%}
        {%- assign half_links_size = half_links_size | plus: 1 -%}
      {%- endif -%}
      @media screen and (min-width: 993px) and (max-width: 1200px) {
        .header__icons-menu--limit-md-8 > .list-menu > li {
          width: {{ 100.00 | divided_by: half_links_size }}%;
        }
      }
    {%- endif -%}
  {%- endstyle -%}
{%- endif -%}

{%- assign contact_block = section.blocks | where: 'type', 'contact' | first -%}

<header-section{% if section.settings.open_onclick %} data-open-onclick="true"{% endif %}>
  <{% if enable_sticky_header %}sticky-header{% else %}div{% endif %} class="header-wrapper gradient sticky-header--type-{{ settings.sticky_header_type | replace: '_', '-' }}"
    {% if enable_sticky_header %} 
      data-enable-desktop="{% if settings.sticky_header_type != 'disable' %}true{% else %}false{% endif %}" 
      data-enable-mobile="{{ settings.sticky_header_enable_mobile }}" 
      data-hide-when-scroll-down="{{ settings.sticky_header_hide_when_scroll_down }}"
      {% if fixed_mega_menu %} 
        data-fixed-menu="true"
      {% endif %}
    {% endif %}
  >
    <header class="header gradient sticky-header__content-mobile{% if settings.sticky_header_type contains 'full' %} sticky-header__content-desktop{% endif %}">
      <div class="header__first-line-wrapper gradient page-width{% if settings.sticky_header_type contains 'text_menu' %} sticky-header__content-desktop{% endif %}">
        <div class="header__first-line header__first-line--{{ settings.header_layout }}{% if section.settings.menu != blank or section.settings.icons_menu != blank %} header__first-line--has-menu{% endif %}">
          <header-drawer data-breakpoint="tablet">
            <details id="Details-menu-drawer-container" class="menu-drawer-container">
              <summary class="header__icon header__icon--menu header__icon--summary link focus-inset" aria-label="{{ 'sections.header.menu' | t }}">
                <span>
                  {% render 'icon-hamburger' %}
                  {% render 'icon-close' %}
                </span>
              </summary>
              <div class="modal-overlay"></div>
              <div id="menu-drawer" class="menu-drawer menu-drawer--type-{{ section.settings.icons_menu_type }} motion-reduce" tabindex="-1">
                <div class="menu-drawer__inner-container">
                  <div class="menu-drawer__inner-container-background">
                    <div class="menu-drawer__navigation-container">
                      <div class="menu-drawer__navigation-heading">
                        <div class="menu-drawer__navigation-button">
                          <button type="button" class="menu-drawer__navigation-button-close js-menu-drawer-back" aria-label="Close menu">
                            {% render 'icon-close' %}
                          </button>
                        </div>
                      </div>
                      {%- if settings.search_enabled and settings.search_mobile_position != 'header' -%}
                        <div class="menu-drawer__navigation-search predictive-search-wrapper">
                          <div class="menu-drawer__search-modal-content-wrapper">
                            {%- if settings.predictive_search_enabled -%}
                              <predictive-search class="search-modal__form" data-loading-text="{{ 'accessibility.loading' | t }}">
                            {%- endif -%}
                            <div class="menu-drawer__search-modal-content">
                              <form action="{{ routes.search_url }}" method="get" role="search" class="search menu-drawer__search-modal-form">
                                <div class="field field--start">
                                  <input class="search__input field__input"
                                    id="Mobile-Search-In-Modal"
                                    type="search"
                                    name="q"
                                    value=""
                                    placeholder="{{ 'general.search.search' | t }}"
                                    {%- if settings.predictive_search_enabled -%}
                                      role="combobox"
                                      aria-expanded="false"
                                      aria-owns="predictive-search-results-list"
                                      aria-controls="predictive-search-results-list"
                                      aria-haspopup="listbox"
                                      aria-autocomplete="list"
                                      autocorrect="off"
                                      autocomplete="off"
                                      autocapitalize="off"
                                      spellcheck="false"
                                    {%- endif -%}
                                  >
                                  <button type="reset" class="reset__button field__button{% if search.terms == blank %} hidden{% endif %}" aria-label="{{ 'general.search.reset' | t }}">
                                    <svg class="icon icon-close" aria-hidden="true" focusable="false">
                                      <use xlink:href="#icon-reset">
                                    </svg>
                                  </button>
                                  <button class="search__button field__button" aria-label="{{ 'general.search.search' | t }}">
                                    <svg class="icon icon-search" aria-hidden="true" focusable="false" role="presentation">
                                      <use href="#icon-search">
                                    </svg>
                                  </button>
                                  <label class="field__label" for="Mobile-Search-In-Modal">{{ 'general.search.search' | t }}</label>
                                  <input type="hidden" name="options[prefix]" value="last">
                                </div>
                              
                                {%- if settings.predictive_search_enabled -%}
                                  <div class="predictive-search predictive-search--mobile predictive-search--header page-width" tabindex="-1" data-predictive-search>
                                    <div class="predictive-search__loading-state">
                                      <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                                        <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                                      </svg>
                                    </div>
                                  </div>
                    
                                  <span class="predictive-search-status visually-hidden" role="status" aria-hidden="true"></span>
                                {%- endif -%}
                              </form>
                            </div>
                            {%- if settings.predictive_search_enabled -%}
                              </predictive-search>
                            {%- endif -%}
                          </div>
                        </div>
                      {%- endif -%}
                      {%- if section.settings.icons_menu != blank -%}
                        <nav class="menu-drawer__navigation">
                          <ul class="menu-drawer__menu menu-drawer__icons-menu list-menu" role="list">
                            {%- assign menu_icons = section.blocks | where: 'type', 'menu_icons' | first -%}
                            {%- comment %}<locksmith:c262>{% endcomment -%}
                              {%- assign locksmith_9c9d_forloop__size = 0 %}{%- for link in section.settings.icons_menu.links -%}{% capture var %}{% render 'locksmith-variables', scope: 'subject', subject: link, subject_parent: section.settings.icons_menu, variable: 'transparent' %}{% endcapture %}{% if var == 'true' %}{% assign locksmith_9c9d_forloop__size = locksmith_9c9d_forloop__size | plus: 1 %}{% endif %}{% endfor %}{% assign locksmith_9c9d_forloop__index = nil -%}
                            {%- comment %}</locksmith:c262>{% endcomment -%}
