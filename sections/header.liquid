<style>
  header-drawer {
    justify-self: start;
    margin-left: -1.2rem;
    z-index: 113;
  }

  @media screen and (min-width: 993px) {
    header-drawer {
      display: none;
    }
  }

  .popup-menu-wrapper {
    display: none;
  }

  
  {%- if section.settings.icons_menu != blank -%}
    {%- if settings.sticky_header_type contains 'full' or settings.sticky_header_type contains 'icons_menu' -%}
      {%- assign fixed_mega_menu = true -%}
    {%- endif -%}
  {%- elsif section.settings.menu != blank -%}
    {%- if settings.sticky_header_type contains 'full' or settings.sticky_header_type contains 'text_menu' -%}
      {%- assign fixed_mega_menu = true -%}
    {%- endif -%}
  {%- endif -%}
  {%- if template.name != 'product' and template.name != 'collection' and request.design_mode != true -%}
    {%- if settings.sticky_header_type != 'disable' or settings.sticky_header_enable_mobile == true -%}
      {%- assign enable_sticky_header = true -%}
    {%- endif -%}
  {%- endif -%}
  {%- assign enable_animated_logo = false -%}
  {%- if settings.sticky_header_type contains 'logo' and template.name == 'index' and settings.enable_logo and request.design_mode != true -%}
    {%- assign enable_animated_logo = true -%}
  {%- endif -%}
  {%- if enable_animated_logo -%}
    #animated-logo-placeholder {
      display: none;
    }
    @media screen and (min-width: 993px) {
      .header__heading  {
        position: relative;
      }

      #animated-logo-placeholder {
        display: block;
        position: absolute;
        left: 0;
        top: 50%;
        transform: translateY(-50%);
        --link-color: var(--inline-menu-color);
        color: rgb(var(--link-color));
        font-family: var(--font-body-family);
        font-size: 1.4rem;
        line-height: calc(1 + 0.285714285714286 / var(--font-body-scale));
        font-weight: 400;
        text-transform: var(--menu-case);
        text-decoration: none;
        white-space: nowrap;
      }

      :is(.header__first-line--one-line-logo-center, .header__first-line--two-lines-logo-center) #animated-logo-placeholder {
        left: 50%;
        transform: translate(-50%, -50%);
      }

      #heading-logo.invisible {
        visibility: hidden;
      }
    }
  {%- endif -%}

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  summary.list-menu__item,
  .header__text-menu-item--has-arrow {
    padding-right: 2.7rem;
  }

  a .header__icons-menu-item-text:not(.header__icons-menu-item-text--has-arrow) .icon-arrow-down,
  a.header__text-menu-item:not(.header__text-menu-item--has-arrow) .icon-caret {
    display: none !important;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: calc(1 + 0.285714285714286 / var(--font-body-scale));
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
  }

  @media screen and (min-width: 577px) {
    .list-menu__item--link {
      padding-bottom: 0.8rem;
      padding-top: 0.8rem;
    }
  }

  {%- if section.settings.background_color.alpha > 0 -%}
    .header__first-line-wrapper {
      background: rgb({{ section.settings.background_color.red }}, {{ section.settings.background_color.green }}, {{ section.settings.background_color.blue }});
    }
  {%- endif -%}

  {%- if section.settings.buttons_color.alpha > 0 -%}
    .header__icon {
      --header-icons-color: {{ section.settings.buttons_color.red }}, {{ section.settings.buttons_color.green }}, {{ section.settings.buttons_color.blue }};
      
    }
    
    .cart-count-bubble {
      --header-cart-count-bubble-color: {% render 'color-contrast' with color: section.settings.buttons_color %};
    }
  {%- endif -%}

  .header__icons-menu,
  .header__menu-popup,
  .popup-menu-wrapper {
    --icons-menu-border-radius: {{ section.settings.icons_menu_border_radius | divided_by: 10.0 }}rem;
  }

  @media screen and (max-width: 992px) {
    .header__heading-svg,
    .header__heading-logo {
      max-width: 100%;
    }
  }
</style>

