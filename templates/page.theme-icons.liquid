<style>
    .theme-icons {
        display: grid;
        grid-template-columns: repeat(3, 1fr);
        margin-top: 8rem;
        padding: 6rem 0;
        border-top: 0.1rem solid rgba(var(--clr-line), var(--clr-line-alpha));
        text-align: center;
    }

    @media screen and (min-width: 577px) {
      .theme-icons {
        grid-template-columns: repeat(5, 1fr);
      }
    }

    @media screen and (min-width: 993px) {
      .theme-icons {
        grid-template-columns: repeat(10, 1fr);
      }
    }

    .theme-icons__icon {
      height: 10.3rem;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      -webkit-transition-duration: .3s;
      transition-duration: .3s;
      -webkit-transition-property: color, background-color;
      transition-property: color, background-color;
      color: rgb(var(--clr-theme-base));
      cursor: pointer;
    }

    .theme-icons__icon svg {
      width: 4rem;
      height: 4rem
    }

    .theme-icons__icon span {
      margin-top: 0.5rem;
    }

    .theme-icons__icon.copied span::after {
      content: ' (Copied)';
    }

    .theme-icons__icon:hover {
      background-color: rgba(var(--clr-theme-base), 0.03);
    }
</style>

