<div class="page-width">
  <h1 class="main-page-title page-title h0 heading-case">
    {{ page.title | escape }}
  </h1>
  <div class="theme-icons">
    {%- for icon_name in icons -%}
      {%- unless icon_name contains 'theme-' -%}
        {%- continue -%}
      {%- endunless -%}
      <div class="theme-icons__icon">
        {% capture icon_html %}{% include icon_name %}{% endcapture %}
        <i>{{ icon_html | replace: '<svg', '<svg width="40" height="40"' }}</i>
        <span>{{ icon_name | remove: 'icon-theme-' }}</span>
      </div>
    {%- endfor -%}
  </div>
</div>

<script>
    const icons = document.querySelectorAll('.theme-icons__icon');

    icons.forEach(element => element.addEventListener('click', function(e) {
      const svg = this.querySelector('span').innerHTML;

      navigator.clipboard.writeText(svg);
      icons.forEach(el => el.classList.remove('copied'));
      this.classList.add('copied');
    }));
</script>